<Window x:Class="Partial_Discharge_Analyzer.FindIPWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Partial_Discharge_Analyzer"
        mc:Ignorable="d"
        Title="Find IP Address" Height="180" Width="300" WindowStartupLocation="CenterScreen">
    <StackPanel>
        <TextBlock Text="Press Find search devices:" Margin="10,10,0,0"/>
        <Button x:Name="FindBtn" Content="Find" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75" Height="22"/>
    </StackPanel>
</Window>
